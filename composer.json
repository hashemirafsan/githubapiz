{
    "name": "hashemirafsan/githubapiz",
    "description": "Github api's",
    "keywords": ["github", "laravel", "PHP", "github-api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hashemi Rafsan",
            "email": "rafsanhashemi@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^6.3@dev"
    },
    "autoload": {
        "psr-4": {
            "HashemiRafsan\\GithubApiz\\": "src/"
        }
    },
    "extra": {
        "laravel" : {
            "providers": [
                "HashemiRafsan\\GithubApiz\\GithubApizServiceProvider"
            ],
            "alias" : {
                "GithubApiz" : "HashemiRafsan\\GithubApiz\\Facades\\GiithubApiz"
            }
        }
    }
}
